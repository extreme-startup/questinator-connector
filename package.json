{
  "name": "questinator-connector",
  "version": "0.1.0",
  "description": "Questinator connector",
  "main": "contender.js",
  "scripts": {
    "start": "node ./bin/questinator.js",
    "publish:pre": "npm version prerelease --force --commit-hooks=false && npm publish",
    "publish:patch": "npm version patch --force --commit-hooks=false && npm publish",
    "publish:minor": "npm version minor --force --commit-hooks=false && npm publish",
    "publish:major": "npm version major --force --commit-hooks=false && npm publish",
    "test": "node ./bin/questinator.js"
  },
  "dependencies": {
    "axios": "0.18.0",
    "prompt": "1.0.0",
    "socket.io-client": "2.2.0"
  },
  "bin": {
    "questinator": "./bin/questinator.js"
  },
  "preferGlobal": true,
  "author": "questinator",
  "license": "MIT"
}
